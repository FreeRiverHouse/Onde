# API Keys for FreeRiver Flow
# Copy this file to .env and fill in your values

# Anthropic API Key (required)
# Used for Claude AI conversation
ANTHROPIC_API_KEY=

# OpenAI API Key (required)
# Used for Whisper speech-to-text transcription
OPENAI_API_KEY=

# ElevenLabs API Key (optional)
# Used for high-quality text-to-speech synthesis
# If not provided, the app will use device TTS
ELEVENLABS_API_KEY=

# ElevenLabs Voice ID (optional)
# Default voice ID to use for TTS
# Find voice IDs at: https://elevenlabs.io/voice-library
ELEVENLABS_VOICE_ID=
