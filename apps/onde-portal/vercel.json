{
  "cleanUrls": true,
  "trailingSlash": true,
  "rewrites": [
    {
      "source": "/games/",
      "destination": "/games/index.html"
    },
    {
      "source": "/games/moonlight-magic-house/",
      "destination": "/games/moonlight-magic-house/index.html"
    }
  ],
  "headers": [
    {
      "source": "/games/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
