    "android": {
      "package": "com.onde.aiko",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/00-cover.jpg",
        "backgroundColor": "#FFF9E6"
      }
    },
    "web": {
      "favicon": "./assets/images/00-cover.jpg"
    },
    "plugins": [
      [
        "expo-av"
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "aiko-interactive-onde"
      }
    }
  }
