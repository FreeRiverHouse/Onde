{
  "expo": {
    "name": "AIKO Interactive",
    "slug": "aiko-interactive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/00-cover.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/00-cover.jpg",
